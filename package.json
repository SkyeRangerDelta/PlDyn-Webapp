    "test:ci": "ng test --browsers ChromeHeadless,FirefoxHeadless --no-watch && eslint .",
    "lint": "ng lint",
    "docker:build": "npm run build && docker build -t pldyn-webapp ."
  },
  "private": false,
  "dependencies": {
